<% include ./partials/_header %>

<h3>My Resources</h3>

<div class="pinnednote   activity">
  <div class="name"></div>
  </div>
  
  <div class="pinned-content">
  <div class="instructions">
  <ul>
      <div><ol>
          <br>
          <% results.forEach(function (result){ %>
            <% var resID = result.id %><br>
            <li><a href= <%= result.url %>><%= result.url %></a> </a> </li>
             <%= result.description + " - " + "Topic: " + result.title %> <br> <a href= <%= '/resource/'+resID %>><%= "Like, Rate or Comment on Resource" %></a><br> 
               <% }) %>
              
      </div></ol>
  
    </ul>
</div>




<% include ./partials/_footer %>

